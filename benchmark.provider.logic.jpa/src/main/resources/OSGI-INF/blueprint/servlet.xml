<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <bean id="reportServlet" class="org.talend.ps.benchmark.provider.ReportServlet">
        <property name="historyIn" ref="eventsHistoryIn"></property>
        <property name="historyOut" ref="eventsHistoryOut"></property>
    </bean>

    <service ref="reportServlet" interface="javax.servlet.http.HttpServlet">
        <service-properties>
            <entry key="alias" value="/report"/>
        </service-properties>
    </service>

</blueprint>
